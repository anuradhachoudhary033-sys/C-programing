#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    float x1,x2,y2,y1;
    float dist;
    
    scanf("%f",&x1);
    
    scanf("%f",&y1);
    
    scanf("%f",&x2);
   
    scanf("%f",&y2);
    dist=sqrt((x2-x1)*(x2-x1)+(y2-y1)*(y2-y1));
    printf("%.2f",dist);
    return 0;
}
